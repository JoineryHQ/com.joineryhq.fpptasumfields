<?xml version="1.0"?>
<extension key="com.joineryhq.fpptasumfields" type="module">
  <file>fpptasumfields</file>
  <name>FPPTA Summary Fields</name>
  <description>Custom Summary Fields for FPPTA</description>
  <license>GPL-3.0</license>
  <maintainer>
    <author>Allen Shaw</author>
    <email>allen@JoineryHQ.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/JoineryHQ/com.joineryhq.fpptasumfields</url>
    <url desc="Documentation">https://github.com/JoineryHQ/com.joineryhq.fpptasumfields</url>
    <url desc="Support">https://github.com/JoineryHQ/com.joineryhq.fpptasumfields/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/gpl-3.0.html</url>
  </urls>
  <releaseDate>2022-12-20</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>Developed for FPPTA by Joinery.<comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Fpptasumfields</namespace>
    <format>22.10.0</format>
    <angularModule>crmFpptasumfields</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
  </mixins>
  <requires>
    <ext>net.ourpowerbase.sumfields</ext>
  </requires>
</extension>
